<?xml version="1.0" encoding="UTF-8"?>
<collectionReaderDescription xmlns="http://uima.apache.org/resourceSpecifier">
        
    <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
        
    <implementationName>de.julielab.jcore.reader.ct.ClinicalTrialsReader</implementationName>
        
    <processingResourceMetaData>
                
        <name>JCoRe ClinicalTrials Reader</name>
                
        <description>Reads the XML format of the Clinical Trial documents from www.clinicaltrials.gov.</description>
                
        <version>unknown</version>
                
        <vendor>de.julielab.jcore.reader.ct</vendor>
                
        <configurationParameters>
                        
            <configurationParameter>
                                
                <name>InputDirectory</name>
                                
                <description/>
                                
                <type>String</type>
                                
                <multiValued>false</multiValued>
                                
                <mandatory>true</mandatory>
                            
            </configurationParameter>
                    
        </configurationParameters>
                
        <configurationParameterSettings>
                        
            <nameValuePair>
                                
                <name>InputDirectory</name>
                                
                <value>
                                        
                    <string>/data/data_corpora/TREC-PM-2018/clinicaltrials/clinicaltrials_xml</string>
                                    
                </value>
                            
            </nameValuePair>
                    
        </configurationParameterSettings>
                
        <typeSystemDescription>
                        
            <imports>
                                
                <import name="de.julielab.jcore.types.jcore-document-structure-clinicaltrial-types"/>
                                
                <import name="de.julielab.jcore.types.jcore-document-meta-clinicaltrial-types"/>
                            
            </imports>
                    
        </typeSystemDescription>
                
        <fsIndexCollection/>
                
        <capabilities/>
                
        <operationalProperties>
                        
            <modifiesCas>true</modifiesCas>
                        
            <multipleDeploymentAllowed>false</multipleDeploymentAllowed>
                        
            <outputsNewCASes>true</outputsNewCASes>
                    
        </operationalProperties>
            
    </processingResourceMetaData>
    
</collectionReaderDescription>
